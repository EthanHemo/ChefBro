<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="johnbryce.com.chefbro">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".RetrieveDataActivity" />
        <activity android:name=".PicturesActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProfileActivity" />
        <activity android:name=".AddRecipeActivity" />
        <activity android:name=".ViewRecipeActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".UpdateProfileActivity" />
        <activity android:name=".UpdateRecipeActivity" />
        <activity android:name=".AddIngredientActivity" />
        <activity android:name=".AddIngredientCategoryActivity" />
        <activity android:name=".AddActionCategoryActivity" />
        <activity android:name=".AddCookingActionActivity"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
